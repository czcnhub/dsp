{
  "type": "VerticalLayout",
  "elements": [
    {
      "type": "Group",
      "label": "Basic Information",
      "elements": [
        {
          "type": "Control",
          "scope": "#/properties/title"
        },
        {
          "type": "Control",
          "scope": "#/properties/description",
          "options": {
            "multi": true
          }
        },
        {
          "type": "HorizontalLayout",
          "elements": [
            {
              "type": "Control",
              "scope": "#/properties/type"
            },
            {
              "type": "Control",
              "scope": "#/properties/additionalTypes"
            }
          ]
        },
        {
          "label": "Keywords",
          "type": "Control",
          "scope": "#/properties/keywords"
        }
      ]
    },
    {
      "type": "Control",
      "scope": "#/properties/status"
    },
    {
      "type": "Control",
      "scope": "#/properties/community"
    },
    {
      "type": "Control",
      "scope": "#/properties/contributors"
    },
    {
      "type": "Control",
      "label": "Spatial Coverage Information",
      "scope": "#/properties/spatialCoverage"
    },
    {
      "type": "Group",
      "label": "Related Resource Information",
      "elements":[
        {
          "type": "Control",
          "scope": "#/properties/identifiers"
        }
      ]
    },
    {
      "type": "Control",
      "scope": "#/properties/fundings"
    },
    {
      "type": "Control",
      "scope": "#/properties/license"
    }
